type User {
    id: ID! @id
    user_name: String!
    userid: String!
    password: String!
    re_passwored: String!
    nickname: String! @unique
    birthday: String!
    gender: String!
    email: String! @unique
    find_email: String
    mailing: String
    phone: Int!
    cellPhone: Int!
    zipCode: Int!
    address: String!
}

type Item {
    id: ID! @id
    item: String!
    quantity: Int!
    price: Int!
    discountedPrice: Int!
    finalPrice: Int!
    shippingFee: Int!
}

